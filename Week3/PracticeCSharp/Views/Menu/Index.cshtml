@using PracticeCSharp.Models.Menu;
@model List<Item>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<h1>Список пунктів меню</h1>

<table class="table">
    @foreach(var item in Model)
    {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.Name
            </td>
            <td>
                @item.Link
            </td>
        </tr>
    }
</table>